@model List<string>

@{
    ViewData["Title"] = "Задание 1";
}

<h1>Задание 1. Рассчитать данные о площадях и объемах каждого помещения.</h1>
<table class="table">
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>@Html.DisplayFor(modelItem => item)</td>
        </tr>
        }
    </tbody>
</table>
